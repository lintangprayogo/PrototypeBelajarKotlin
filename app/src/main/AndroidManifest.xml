<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prototypebelajarkotlin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PrototypeBelajarKotlin">
        <activity
            android:name=".QuizActivity"
            android:theme="@style/Theme.PrototypeBelajarKotlin.NoActionBar" />
        <activity
            android:name=".MateriDetailActivity"
            android:theme="@style/Theme.PrototypeBelajarKotlin.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.PrototypeBelajarKotlin.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TanyaActivity"
            android:theme="@style/Theme.PrototypeBelajarKotlin.NoActionBar" />
        <activity
            android:name=".JawabActivity"
            android:theme="@style/Theme.PrototypeBelajarKotlin.NoActionBar" />
        <activity
            android:name=".ForumActivity"
            android:theme="@style/Theme.PrototypeBelajarKotlin.NoActionBar" />
        <activity
            android:name=".MateriActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.PrototypeBelajarKotlin.NoActionBar" />
    </application>

</manifest>